func (m *<%= type.name %>) PrepareToCreate() error {
  m.PrepareFields()
<%- if type.fields.any?{|f| f.name == "CreatedAt"} -%>
<%-   import "time" -%>
	if m.CreatedAt.IsZero() {
		m.CreatedAt = time.Now()
	}
<%- end -%>
<%- if type.fields.any?{|f| f.name == "UpdatedAt"} -%>
<%-   import "time" -%>
	if m.UpdatedAt.IsZero() {
		m.UpdatedAt = time.Now()
	}
<%- end -%>
	return nil
}
